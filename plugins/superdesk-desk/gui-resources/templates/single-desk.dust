<div sd-modal class="split-popup" id="AddAssignment" ng-controller="EditTaskController">
  <form name="taskForm">
  <div class="modal-header">
    <a href="" class="close" data-action="close" data-dismiss="modal" aria-hidden="true">&times;</a>
    <input type="text" placeholder="_('Assignment title');" class="assignment-title" data-task-info="title" ng-model="task.Title" required />
  </div>
  <div class="modal-body">
    <div class="modal-left-pane">
        <div class="taskmodal-header">
        	<div class="parent-task-info">
        		_('Parent task:'); <a href="" ng-show="parentTask.Title"><span data-task-info="parent">{{ parentTask.Title }}</span></a>
        	</div>
        	<ul class="assign-labels clearfix" style="margin-top:5px">
				<li class="critical" ng-show="task.DueDate">
					<i class="icon-desk-time icon-white"></i> <span data-task-info="due-date" sd-date-time="task.DueDate"></span>
				</li>
				<li ng-show="task.Id">
					<i class="icon-desk-subtask icon-white"></i> <span>{{ subtasks.length }}</span> <span ng-pluralize count="subtasks.length" when="{'one': '_("Subtask");', 'other': '_("Subtasks");'}"></span> 
				</li>
			</ul>
        </div>
        <div class="taskmodal-content clearfix">
        	<textarea class="taskmodal-description" ng-model="task.Description" placeholder="_("Task description");" ></textarea>

        	<section class="modal-mainpane-module" ng-show="task.Id">
        		<header><span>_('Subtasks');</span></header>
        		<ul class="subtask-list-view clearfix">
        			<li ng-repeat="subtask in subtasks" class="clearfix">
        				<h6>{{ subtask.Title }}</h6>
        				<p>{{ subtask.Description }}</p>
        				<span class="assign-label critical" ng-show="subtask.DueDate"><i class="icon-desk-time icon-white"></i> <span sd-date-time="subtask.DueDate"></span></span>
                        <a href="" ng-click="editTask(subtask, task)">_("Open details");</a>
        			</li>
        		</ul>
        	</section>

            <section class="modal-mainpane-module" ng-show="task.Id">
                <header>
                    <span>_('Attached files');</span>
                    <div class="module-header-menu">
                        <a href="#AddFile" ng-click="addFile()" data-toggle="modal"><i class="icon-plus"></i> _('Attach file');</a>
                    </div>
                </header>
                <ul class="attached-files-view">
                    <li ng-repeat="file in task_files">
                        <figure><img ng-src="{{ file.Thumbnail.href }}" alt="{{ file.Name }}"></figure>
                        <h6>{{ file.Name }}</h6>
                        <div class="attached-file-info">
                            <span>{{ file.CreatedOn }}</span>
                            <span>{{ file.Type }}, {{ file.SizeInBytes }}B</span>
                        </div>
                        <div class="action-menu">
                            <a href="" ng-click="removeFile(task_files, $index)"><i class="icon-trash"></i> _('Remove');</a>
                        </div>
                    </li>
                    <li ng-show="!task_files.length">_("No files yet");</li>
                </ul>
            </section>

            <section class="modal-mainpane-module" id="linksmodule" ng-show="task.Id">
        		<header>
        			<span>_('Links');</span>
        			<div class="module-header-menu">
        				<a class="" id="linkbtn2" ng-click="addLink(task_links, task)"><i class="icon-plus"></i>_('Add Link');</a>
        			</div>
        		</header>
        		<ul class="links-view clearfix">
        			<li ng-repeat="link in task_links" ng-init="ui.edit=!link.URL">
        				<h6 class="linkdesc" ng-hide="ui.edit">{{ link.Title }}</h6>
        				<a class="linkurl" ng-hide="ui.edit" ng-href="{{ link.URL }}" target="_blank">{{ link.URL }}</a>
        				<fieldset ng-show="ui.edit">
		    				<input type="text" class="linkdesc" placeholder="_('Link Description');" ng-model="link.Title" >
		    				<input type="text" class="linkurl" placeholder="_('Link Url');" ng-model="link.URL" >
		    			</fieldset>
        				<div class="action-menu">
        					<a href="" class="okbtn" ng-show="ui.edit" ng-click="ui.edit=false"><i class="icon-ok"></i> _('Ok');</a>
        					<a href="" class="edtbtn" ng-click="ui.edit=true" ng-hide="ui.edit"><i class="icon-pencil"></i> _('Edit');</a>
		    				<a href="" ng-click="removeLink(task_links, $index)"><i class="icon-trash"></i> _('Delete');</a>
		    			</div>
        			</li>
                    <li ng-show="!task_links.length">_("No links yet");</li>
        		</ul>
        	</section>    

        	<section class="modal-mainpane-module" ng-show="task.Id">
                <header><span>_('Comments');</span></header>
                <div class="add-comment-box" ng-class="{focused:addComment}">
                    <textarea id="add-comment" placeholder="_('Write a comment');" ng-model="commentText" ng-click="addComment = true"></textarea>
                    <a class="btn btn-mini" ng-click="commentText = ''; addComment = false">_('Cancel');</a>
                    <a class="btn btn-primary btn-mini" ng-click="saveComment(commentText)" ng-disabled="!commentText">_('Send Comment');</a>
                </div>

                <ul class="comment-list" ng-show="comments.length > 0">
                    <li ng-repeat="comment in comments">
                        <div class="comment-info">
                            <figure sd-user-image sd-user="comment.User" data-width="32" data-height="32"></figure>
                            <b>{{ comment.User.Name }}</b>
                            <time>{{ comment.UpdatedOn }}</time>
                        </div>
                        <div ng-hide="edit">
                            <p class="comment-text">{{ comment.Text }}</p>
                            <div class="action-menu">
                                <a href="" ng-click="edit = true; oldComment = comment.Text"><i class="icon-pencil"></i> _('Edit');</a>
                                <a href="" ng-click="deleteComment($index)"><i class="icon-trash"></i> _('Delete');</a>
                            </div>
                        </div>
                        <div ng-show="edit" class="add-comment-box focused">
                            <textarea placeholder="_('Write a comment');" ng-model="comment.Text"></textarea>
                            <a class="btn btn-mini" ng-click="comment.Text = oldComment; edit = false">_('Cancel');</a>
                            <a class="btn btn-primary btn-mini" ng-click="saveComment(comment); edit = false">_('Save Comment');</a>
                        </div>
                    </li>
                </ul>
            </section>
        </div>
    </div>
    <div class="modal-right-pane">
    	<section class="modal-sidepane-module clearfix">
    		<header ng-show="task.User">_('Assignee');</header>
    		<div class="module-sidepane-module-content">
    			<div class="assignee-info" ng-show="task.User" data-task-info="assignee">
   					<figure sd-user-image sd-user="task.User" data-width="32" data-height="32"></figure>
   					<b data-task-info="assignee-name">{{ task.User.Name }}</b>
    			</div>
    			<div class="btn-group">
				  <a class="btn dropdown-toggle" data-toggle="dropdown" href="">
				    <i class="icon-plus-sign"></i> _('Assign');
				    <span class="caret"></span>
				  </a>
				  <ul class="dropdown-menu sf-dropdown sf-dropdown-right" data-list="users">
					<li ng-repeat="user in users" ng-click="task.User=user">{{ user.Name }}</li>
				  </ul>
				</div>
    		</div>
    	</section>
    	<section class="modal-sidepane-module clearfix">
    		<header>_('Status');</header>
    		<div class="module-sidepane-module-content">
                <div class="status-info" ng-show="task.Status">
                    <b>{{ task.Status.Key }}</b>
                </div>
    			<div class="btn-group">
				  <a class="btn dropdown-toggle" data-toggle="dropdown" href="">_('Status');<span class="caret"></span></a>
				  <ul class="dropdown-menu sf-dropdown sf-dropdown-right" data-list="users">
					<li ng-repeat="status in statuses" ng-click="task.Status=status">{{ status.Key }}</li>
				  </ul>
				</div>
    		</div>
    	</section>
    	<section class="modal-sidepane-module clearfix">
    		<header>_('Action');</header>
    		<div class="module-sidepane-module-content">
    			<input type="text" ng-model="task.DueDate" sd-datepicker />
    			<a class="btn" href="" data-toggle="datepicker"><i class="icon-desk-time"></i> _('Due Date');</a>
    			<a class="btn" href="" ng-click="addTask(desk, orig)" ng-show="task.Id"><i class="icon-desk-subtask"></i>_('Add Subtask');</a>
    			<a class="btn" href="" style="margin-top:30px;" ng-show="task.Id" ng-click="deleteTask(tasks)" data-dismiss="modal"><i class="icon-trash"></i> _('Delete Task');</a>
    		</div>
    	</section>
    </div>
  </div>
  <div class="modal-footer">
    <button type="button" class="btn" data-dismiss="modal">_('Cancel');</button>
    <button type="button" class="btn btn-primary" data-dismiss="modal" ng-click="saveTask()" ng-disabled="!taskForm.$valid">_('Save Changes');</button>
  </div>
  </form>
</div>

<script type="text/ng-template" id="media-search">
    <input class="span3 searchbox" size="16" type="text" name="search" placeholder="Search Media Archive" style="margin:0;float:left !important;margin-right:20px;" ng-model="query">
    <label class="checkbox label-inline"><input type="checkbox" ng-model="is_my"> Show only my uploads</label>
    <ul class="uploads-grid" style="min-height:300px">
        <li ng-repeat="item in items" title="{{ item.Name }}" ng-class="{'grid-selected':item.selected}" ng-click="item.selected = !item.selected">
            <div class="add-button"><i class="icon-white" ng-class="{'icon-plus': !item.selected, 'icon-minus': item.selected}"></i></div>
            <img ng-src="{{ item.Thumbnail.href }}" />
        </li>
    </ul>
</script>

<div sd-modal class="responsive-popup" id="AddFile" ng-controller="AttachFileController" data-modal-open="reset()">
  <div class="modal-header">
    <a href="#" class="close" data-dismiss="modal" aria-hidden="true" ng-click="reset()">&times;</a>
    <h3>Add Image</h3>
  </div>
  <div class="modal-body">

    <div class="modal-left-pane">
        <h6>Sources</h6>
        <ul class="nav-pane-menu nav">
          <li class="active"><a href="#upload" data-toggle="tab">From Computer</a></li>
          <li><a href="#archive" data-toggle="tab" ng-click="searchMedia(task)">From Media Archive</a></li>
        </ul>
    </div>

    <div class="modal-right-pane tab-content">

      <div class="uploadimage-screen tab-pane active" id="upload">
        <div class="drag-area" sd-upload>
          <div class="drag-area-title">
            Drag Your Images Here or Click to select them from folder
          </div>
        </div>
      </div>

      <div class="uploadimage-screen tab-pane" id="archive" ng-include="'media-search'">
      </div>

    </div>
  </div>
  <div class="modal-footer">
    <a href="" class="btn" data-dismiss="modal" ng-click="reset()">Cancel</a>
    <a href="" class="btn btn-primary" data-dismiss="modal" ng-click="save(task)">OK</a>
  </div>
</div>
