		                    {{ card.Name }}
	                    </div>
	                    <div class="desk">
		                    <div class="desk-content">
                                <div class="board-statuses">
                                    <ul>
                                        <li ng-repeat="status in card.statuses" sd-droppable ng-model="status"><span>{{ status.Key }}</span></li>
                                    </ul>
                                </div>
			                    <ul class="assignments" style="position:relative">
                                    <li ng-repeat="task in tasks | filter:isCardTask | filter:isMyTask" sd-draggable ng-model="task" class="task-draggable" ng-controller="TaskController">
                                        <figure class="assign-avatar" sd-user-image data-width="20" data-height="20" sd-user="task.User"></figure>
                                        <div class="assign-description">
                                            <a href="#AddAssignment" ng-click="editTask(task)" data-toggle="modal">{{ task.Title }}</a>
                                        </div>
                                        <ul class="assign-labels clearfix">
	                                        <li><i class="icon-desk-time icon-white"></i> <span sd-date-time="task.DueDate"></span></li>
	                                        <li><i class="icon-desk-task icon-white"></i> {{ task.subtasks.length }}</li>
	                                        <li><i class="icon-desk-comments icon-white"></i> {{ task.comments.length }}</li>
                                        </ul>
                                    </li>
                                    <li ng-show="isEmpty(tasks)">_("This card has no tasks yet.");</li>
                                </ul>
		                    </div>
	                    </div>
                    </div>
                    <div ng-show="!cards.length"><h3>_("This desk has no cards yet.");</h3></div>

