		<div class="main-desk-container" id="main-desk" style="margin-left:auto;margin-right:auto;max-width:960px" ng-class="{'compact-desk': compact}">
				<div class="desk-content-container" ng-class="{'desk-list': list}">
					<div class="desk-horizontal-scroll" style="width:870px!important">
					<div class="desk-container" ng-repeat="desk in desks" ng-controller="DeskController">
						<div class="desk-header"><a ng-href="#/desks/{{ desk.Id }}">{{ desk.Name }}</a></div>
						<div class="desk">
								<section class="boardcolor-{{ card.Color }}" ng-repeat="card in cards" ng-controller="CardController">
									<header>{{ card.Name }}</header>
									<ul class="assignments">
										<li class="task-draggable" ng-repeat="task in tasks | filter:isCardTask" ng-controller="TaskController">
											<figure class="assign-avatar" sd-user-image sd-user="task.User" data-width="20" data-height="20"></figure>
											<div class="assign-description">{{ task.Title }}</div>
											<ul class="assign-labels clearfix">
												<li><i class="icon-desk-time icon-white"></i> {{ task.DueDate }}</li>
												<li><i class="icon-desk-task icon-white"></i> {{ task.subtasks.length }}</li>
												<li><i class="icon-desk-comments icon-white"></i> {{ task.comments.length }}</li>
											</ul>
											<ul class="subtasks" ng-show="task.subtasks.length" ng-class="{open: open}">
												<div class="subtask-num-info" ng-click="open=!open"><span>{{ task.subtasks.length }}</span></div>
												<li ng-repeat="subtask in task.subtasks">{{ subtask.Title }}</li>
											</ul>
										</li>
                                        <li ng-show="isEmpty(tasks)">_("This card has no tasks yet.");</li>
									</ul>
								</section>

                                <section ng-show="!cards.length"><header>_("This desk has no cards yet.");</header></section>
						</div>
					</div>
				</div>
			</div>
		</div>




